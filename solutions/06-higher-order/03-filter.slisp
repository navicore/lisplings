; === filter: Select Elements ===

(define result (filter (lambda (x) (equal? (modulo x 2) 0)) (list 1 2 3 4 5 6)))

;; ===== Tests - do not edit below this line =====
(define (assert-eq actual expected msg)
  (if (equal? actual expected)
      #t
      (begin (print (list 'FAIL msg 'expected expected 'got actual))
             (exit 1))))

(assert-eq result (list 2 4 6) "result should be (2 4 6)")
(exit 0)
